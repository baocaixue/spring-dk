<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- remove attribute 'primary' to cause an UnsatisfiedDependencyException whe runnign the example -->
    <bean id="fooOne" class="com.isaac.ch3.xml.complicated.FooImpl1" primary="true"/>
    <bean id="fooTwo" class="com.isaac.ch3.xml.complicated.FooImpl2"/>

    <bean id="bar" class="com.isaac.ch3.xml.Bar"/>

    <bean id="targetByType" autowire="byType" class="com.isaac.ch3.xml.complicated.CTarget"
        lazy-init="true"/>

</beans>
